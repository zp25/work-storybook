{
  "name": "work-storybook",
  "version": "0.0.0",
  "description": "Storybook React starter kit",
  "main": "index.js",
  "scripts": {
    "upgrade-interactive": "npm-check -u",
    "start": "start-storybook --port 9000 -c .storybook",
    "eslint": "eslint --ext .js,.jsx .",
    "stylelint": "stylelint \"src/**/*.{scss,css}\" \"assets/**/*.{scss,css}\"",
    "lint": "npm run eslint && npm run stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{scss,css}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zp25/work-storybook.git"
  },
  "author": "zp25",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "spinkit": "^2.0.1",
    "zp-lib": "^0.2.0",
    "zp-ui": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "node-sass": "^7.0.0",
    "npm-check": "^5.9.0",
    "sass-loader": "^8.0.0",
    "stylelint": "^12.0.0",
    "stylelint-config-sass-guidelines": "^6.1.0",
    "stylelint-webpack-plugin": "^1.1.0"
  },
  "engines": {
    "node": ">=8.16.0"
  }
}
